						<sci:member>Keep Calculated Properties</sci:member>
						<sci:value>RESIZE(SALTCOEFF, NumValues(SALTNAME));
FOR #partnum in 1 .. NumValues(SALTNAME)
LOOP
	#index := assignedsalt[#partnum];
	IF #index &lt; 1
	THEN
		errorc := &apos;Bad salt Assignment for &apos; . SALTNAME[#partnum];
	ELSE
		SALTCOEFF[#partnum] := MOLFcoeff[#index];
	END IF;
END LOOP;
	</sci:value>
