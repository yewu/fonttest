						<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
						<sci:member>Initial Expression</sci:member>
						<sci:member>Final Expression</sci:member>
						<sci:member>Keep Calculated Properties</sci:member>
						<sci:value>RESIZE(MOLFWgts,NumValues(MOLFParts));
FOR #partnum in 1 .. NumValues(MOLFParts)
LOOP
	MOLFWgts[#partnum] := -10000.0;
	#thiswgt := 0.0;
	#matched := RMatch(MOLFParts[#partnum], &quot;[A-Z][a-z]?&quot;, &apos;g&apos;);
	#lastend :=1;
	FOR #elenum in 1 .. NumValues(#matched)
	LOOP
		#sidx := FindSubString(MOLFParts[#partnum],#matched[#elenum],#lastend);
		IF #elenum &lt; NumValues(#matched)
		THEN
			#endnum := FindSubString(MOLFParts[#partnum],#matched[#elenum + 1],#lastend + 1);
			#lastend := #endnum;
			#sslen := #endnum - #sidx;
			#elepart := SubString(MOLFparts[#partnum],#sidx, #sslen);
		ELSE
			#elepart := SubString(MOLFparts[#partnum],#sidx);
		END IF;
		IF StrLength(#elepart) &lt; 2
		THEN
			#elecnt := 1;
			#elesym := #elepart;
		ELSE
			#elecnt := SubString(#elepart,2);
			IF ISINTEGER(#elecnt)
			THEN
				#elesym := SubString(#elepart,1,1);
			ELSE
			#elesym := SubString(#elepart,1,2);
			IF StrLength(#elepart) &gt; 2
				THEN
					#elecnt := SubString(#elepart,3);
				ELSE
					#elecnt := 1;
				END IF;
			END IF;
		END IF;
		IF ISINTEGER(#elecnt)
		THEN
			#thiswgt := #thiswgt + #elecnt * ElementAtomicWeight(ElementNumber(#elesym));
		ELSE
			#thiswgt := -100000.0;
		END IF;
	END LOOP;
	IF #thiswgt &gt; 0.0
	THEN
		MOLFWgts[#partnum] := #thiswgt;
	END IF;
END LOOP;

#numwgts := NUMValues(MOLFWgts);
IF #numwgts &gt; 1
THEN
	minwgtdiff := 10;
	FOR #idx1 in 2 .. #numwgts
	LOOP
		FOR #idx2 in 1 .. #idx1 - 1
		LOOP
			#tdiff := ABS(MOLFWgts[#idx1] - MOLFWgts[#idx2]);
			IF #tdiff &lt; minwgtdiff
			THEN
				minwgtdiff := #tdiff;
			END IF;
		END LOOP;
	END LOOP;
END IF;

minwgtdiff := minwgtdiff/2;
			
</sci:value>
