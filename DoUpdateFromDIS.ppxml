Parameter(&apos;Authentication Method&apos;) eq &apos;Use Username and Password Parameters&apos; and
	Parameter(&apos;Data Source&apos;) not rlike &apos;.;PWD=[^;]+(;|$)&apos;;</sci:enabledscript>
