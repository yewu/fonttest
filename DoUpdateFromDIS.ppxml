					<sci:data object="SciTegic.EvaluateExpression.1">
						<sci:component name="Custom Manipulator (PilotScript)" version="2">
							<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
								<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
								<sci:member>Initial Expression</sci:member>
								<sci:member>Final Expression</sci:member>
								<sci:member>Keep Calculated Properties</sci:member>
								<sci:value>/** split MOLF into parts by looking for the dot 
    use The DIS SALTMF if it is defined, else the MOLF **/
IF DISSALTMF2 IS DEFINED
THEN
    FULLMOLF := DISSALTMF;
	#parts := RSplit(DISSALTMF, &apos;\.&apos;);
ELSE
	FULLMOLF := MOLF;
	#parts := RSplit(MOLF, &apos;\.&apos;);
END IF;
HFlag := FALSE;
Form_Num_Fragments := NumValues(#parts);
RESIZE(MOLFparts,Form_Num_Fragments);
RESIZE(MOLFcoeff,Form_Num_Fragments);

/** Loop over all the parts **/
FOR #partnum in 1 .. Form_Num_Fragments
LOOP
	#tpart := TRIM(#parts[#partnum]);
	/** look for the first letter **/
	#matched := RMatch(#tpart, &quot;[A-Z]&quot;);
	#sidx := FindSubString(#tpart, #matched);
	/** if the letter is at the start of the string, the coefficient is 1 **/
	If #sidx == 1
	THEN
		MOLFcoeff[#partnum] := 1.0;
		/** get rid of any spaces **/
		MOLFparts[#partnum] := RSubst(#tpart, &apos; &apos;, &apos;&apos;, &apos;g&apos;);
		/** if the fragment is only a H, it is a kludge and not a real fragment **/
		IF MOLFparts[#partnum] EQ &apos;H&apos;
		THEN
			HFlag := TRUE;
			Form_Num_Fragments := Form_Num_Fragments - 1;
		END IF;
	ELSE
		/** if a letter isn&apos;t the first character, it must start with a coefficient **/
		#sCoeff := Trim(Substring(#tpart,1, #sidx - 1));
		#ttpart := Substring(#tpart,#sidx);
		MOLFparts[#partnum] := RSubst(#ttpart, &apos; &apos;, &apos;&apos;, &apos;g&apos;);
		IF MOLFparts[#partnum] EQ &apos;H&apos;
		THEN
			HFlag := TRUE;
			Form_Num_Fragments := Form_Num_Fragments - 1;
		END IF;
		/** look for coefficient like x/y **/
		#cparts := RSplit(#sCoeff, &apos;/&apos;);
		IF NumValues(#cparts) == 1
		THEN
			MOLFcoeff[#partnum] := Double(#sCoeff);
		ELSIF NumValues(#cparts) == 2
		THEN
			MOLFcoeff[#partnum] := TRIM(#cparts[1])/TRIM(#cparts[2]);
		ELSE
			MOLFcoeff[#partnum] := -1.0;
		END IF;
	END IF;
END LOOP;
HCheck := &apos;false&apos;;
IF HFlag
THEN
	HCheck := &apos;true&apos;;
END IF;</sci:value>
