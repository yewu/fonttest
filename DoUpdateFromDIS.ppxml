								<sci:member>AcceptNonNumericStringsAsNumbers</sci:member>
								<sci:member>IgnoreCase</sci:member>
								<sci:value>NSC, MW, MOLF, Form_Num_Fragments, MOLFparts, MOLFcoeff, MOLFWgts, removedsolvents</sci:value>
