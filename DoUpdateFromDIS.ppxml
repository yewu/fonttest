					<sci:data object="SciTegic.EvaluateExpression.1">
						<sci:component name="Custom Manipulator (PilotScript)" version="2">
							<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
								<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
								<sci:member>Initial Expression</sci:member>
								<sci:member>Final Expression</sci:member>
								<sci:member>Keep Calculated Properties</sci:member>
								<sci:value>#NonStandard := 0;
#ToDelete := 0;
RESIZE(#DelAtoms, 0);
For #Atom in 1 .. MolNumAtoms() 
Loop
	#NSLength := StringLen(AtomNonstandardType(#Atom));
	IF ( AtomHasNonStandardType(#Atom) ) OR (AtomTypeString(#Atom) EQ &apos;R#&apos; ) OR ( #NSLength &gt; 0 )
	THEN
		IF AtomNumAtt(#Atom) == 0
		THEN
			Append(#DelAtoms, #Atom);
			#ToDelete := #ToDelete + 1;
		ELSE
			#NonStandard := #NonStandard + 1;
		END IF;
	END IF;
	IF ( AtomTypeString(#Atom) EQ &quot;Na&quot; ) OR 
		( AtomTypeString(#Atom) EQ &quot;K&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Li&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Hg&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Au&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Cs&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Tl&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Rb&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Ag&quot; )
	THEN
		IF AtomNumAtt(#Atom) == 0 
		THEN
			AtomSetCharge(#Atom, 1);
		END IF;
	END IF;
	IF ( AtomTypeString(#Atom) EQ &quot;Ba&quot; ) OR 
		( AtomTypeString(#Atom) EQ &quot;Be&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Cd&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Ca&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Co&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Mg&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Zn&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Cu&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Ni&quot; )
	THEN
		IF AtomNumAtt(#Atom) == 0 
		THEN
			AtomSetCharge(#Atom, 2);
		END IF;
	END IF;
	IF ( AtomTypeString(#Atom) EQ &quot;Al&quot; ) OR 
		( AtomTypeString(#Atom) EQ &quot;Sb&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Ga&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Bi&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;Ce&quot; ) OR
		( AtomTypeString(#Atom) EQ &quot;La&quot; )
	THEN
		IF AtomNumAtt(#Atom) == 0 
		THEN
			AtomSetCharge(#Atom, 3);
		END IF;
	END IF;


END LOOP;
BadAtomType := #NonStandard;
DeletedAtoms := 0;
IF #ToDelete &gt; 0
THEN
	MolDeleteAtoms(#DelAtoms);
	DeletedAtoms := #ToDelete;
END IF;
</sci:value>
