						<sci:value>RESIZE(SOLVENTCOEFF, NumValues(SOLVENTNAME));
FOR #partnum in 1 .. NumValues(SOLVENTNAME)
LOOP
	#index := assignedsolvent[#partnum];
	IF #index &lt; 1
	THEN
		errorc := &apos;Bad solvent Assignment for &apos; . SOLVENTNAME[#partnum];
	ELSE
		SOLVENTCOEFF[#partnum] := MOLFcoeff[#index];
	END IF;
END LOOP;
	</sci:value>
