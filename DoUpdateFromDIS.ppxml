										<sci:value>IF DISSALTMW IS DEFINED
THEN
	#DISCHECK := DISSALTMW;
ELSE
	#DISCHECK := MW;
END IF;

#CheckMW := ABS(FullMW + AddSolventMW - #DISCHECK);
IF #CheckMW &lt; 0.8
THEN
	DISMWTEST := &apos;Passed&apos;;
ELSIF #CheckMW &lt; 6.5
THEN
	DISMWTEST := &apos;Small Difference&apos;;
ELSE
	DISMWTEST := &apos;Large Difference&apos;;
END IF;
	


</sci:value>
